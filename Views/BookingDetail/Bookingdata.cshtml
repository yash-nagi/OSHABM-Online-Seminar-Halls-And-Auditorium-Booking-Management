@model IEnumerable<OSHABM_proj.Models.Booking>

@{
    ViewBag.Title = "Bookingdata";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- header section start -->
<div class="header_section">
    <nav class="destop_header navbar navbar-expand-lg navbar-light bg-light">
        <div class="logo"></div>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="logo_main" href="@Url.Action("Index","Welcome")">
                        <img src="~/Content/images/OSHABM logo.png" height="100" width="200" />
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" @Html.ActionLink("Home", "Index", "Welcome")</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" @Html.ActionLink("About", "Index", "Welcome")</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" @Html.ActionLink("Services", "Index", "Welcome")</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" @Html.ActionLink("Images", "Index", "Welcome")</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" @Html.ActionLink("Contact", "Index", "Welcome")</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Login","Welcome")">
                        <img src="~/Content/images/search-icon.png">
                    </a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" @Html.ActionLink("LOGIN", "Login", "Welcome")</a>
                </li>
            </ul>
        </div>
    </nav>
</div>
<!-- header section end -->
<div style="background-color:antiquewhite">
    <h2>Bookingdata</h2>

    <p>
        @*@Html.ActionLink("Create New", "Create")*@
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmailID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AuditId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date_of_Booking)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Time_of_Booking)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PayId)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EmailID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AuditId)
                </td>
                <td>
                    @{
                        string datevalue = Convert.ToDateTime(item.Date_of_Booking).ToShortDateString();
                    }
                    @datevalue
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Time_of_Booking)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PayId)
                </td>
                <td>

                    @*@Html.ActionLink("Edit", "Edit", new { id=item.BookingId }) |
                        @Html.ActionLink("Details", "Details", new { id=item.BookingId }) |
                        @Html.ActionLink("Delete", "Delete", new { id=item.BookingId })*@
                </td>
            </tr>
        }

    </table>
    @Html.ActionLink("Go Back to Admin portal", "AdminPage", "StaffDetail", null, new { @style = "color:#000aaa" })
</div>
